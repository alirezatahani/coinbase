{
  "manifest_version": 3,
  "name": "coinRanking",
  "description": "coinbase chrome extension",
  "version": "0.0.1",
  "action": {
    "default_popup": "index.html",
    "default_title": "Open"
  },
  "icons": {
    "16": "./logo1.png"
  },
  "permissions": ["storage", "activeTab", "scripting", "tabs"],
  "host_permissions": ["http://*/*", "https://*/*"]
}
